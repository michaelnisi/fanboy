{
  "name": "fanboy",
  "version": "7.0.3",
  "description": "Caching iTunes search proxy",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "postpublish": "git push origin --all; git push origin --tags",
    "posttest": "rm -rf /tmp/fanboy-[1-9]*",
    "postversion": "npm publish",
    "pretest": "standard --fix",
    "preversion": "npm test",
    "test": "tap test/*.js -b -j1"
  },
  "dependencies": {
    "JSONStream": "^1.3.5",
    "charwise": "^3.0.1",
    "level": "^5.0.1",
    "level-random": "^4.0.3",
    "lru-cache": "^4.0.1"
  },
  "devDependencies": {
    "@types/node": "^12.0.8",
    "nock": "^11.4.0",
    "rimraf": "^2.6.1",
    "standard": "^14.3.1",
    "stread": "^3.0.1",
    "tap": "^14.6.9"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/michaelnisi/fanboy.git"
  },
  "keywords": [
    "proxy",
    "cache",
    "itunes",
    "search",
    "api"
  ],
  "author": "Michael Nisi (https://troubled.pro)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/michaelnisi/fanboy/issues"
  },
  "homepage": "https://github.com/michaelnisi/fanboy",
  "engines": {
    "node": ">=10"
  }
}
